{
    "root": true,
    "env": {
        "node": true
    },
    "parser": "vue-eslint-parser",
    "plugins": [
        "prettier"
    ],
    "extends": [
        "plugin:vue/vue3-essential",
        "eslint:recommended",
        "@vue/typescript/recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "parser": "@typescript-eslint/parser",
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "warn",
        "no-debugger": "warn"
    }
}